@page
@model Mined.Pages.Admin.Uxos.CreateModel

<div class="container-xl px-4 mt-4">
	<div asp-validation-summary="All"></div>
	<form method="post">
		<div class="col-xl-12">
			<!-- Add U.X.O -->
			<div class="card mb-4">
				<div class="card-header">Add U.X.O. Required Information</div>
				<div class="card-body">
					
					<div class="row gx-12 mb-12">
						<!--NATO U.X.O. Name-->
						<div class="form-group col-md-6">
							<label asp-for="@Model.Uxo.NameNato">NATO U.X.O. name</label>
							<input asp-for="@Model.Uxo.NameNato" class="form-control" />
							<span asp-validation-for="Uxo.NameNato" class="text-danger"></span>
						</div>

						<div class="form-group col-md-6">
							<label asp-for="@Model.Image.UxoImage">Upload U.X.O. Image</label>
							<input asp-for="@Model.Image.UxoImage" class="form-control" />
							<span asp-validation-for="@Model.Image.UxoImage" class="text-danger"></span>
						</div>
					</div>

					<!-- NATO U.X.O. Categories-->
					<div class="row mb-12">
						<!-- NATO U.X.O. Main Category-->
						<div class="form-group col-md-6">
							<!--Hiervan een listbox maken-->
							<label asp-for="Category.MainCategoryNato">NATO U.X.O. Main Category</label>
							<input asp-for="Category.MainCategoryNato" class="form-control" />
							<span asp-validation-for="Category.MainCategoryNato" class="text-danger"></span>
						</div>

						<!-- NATO U.X.O. Subcategory-->
						<div class="form-group col-md-6">
							<!--Hiervan een listbox maken-->
							<label asp-for="Category.SubCategoryNato">NATO U.X.O. Subcategory</label>
							<input asp-for="Category.SubCategoryNato" class="form-control" />
							<span asp-validation-for="Category.SubCategoryNato" class="text-danger"></span>
						</div>
					</div>
				</div>
			</div>
		</div>
	</form>
</div>

